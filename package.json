{
  "name": "auth-demo",
  "version": "1.16.1",
  "main": "index.js",
  "author": "ArcBlock",
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "lint": "eslint src api",
    "clean": "rm -rf build",
    "start": "npm-run-all --parallel start:*",
    "start:client": "NODE_ENV=development react-app-rewired start",
    "start:server": "nodemon api/index.js -w api",
    "bundle": "yarn clean && npm run bundle:client && npm run bundle:server",
    "bundle:client": "REACT_APP_API_PREFIX=\"\" REACT_APP_APP_NAME=\"Auth Demo\" yarn build:client",
    "bundle:server": "blocklet bundle --create-release",
    "build:client": "react-scripts build && rm -f build/**/**/*.{js,css}.map",
    "test": "node tools/jest.js",
    "release": "npm run clean && npm run bundle",
    "coverage": "npm run test -- --coverage",
    "update:deps": "npx taze -w && npm run dedupe",
    "dedupe": "npx yarn-deduplicate yarn.lock && yarn",
    "bump-version": "make bump-version"
  },
  "dependencies": {
    "@arcblock/did-connect": "^2.9.1",
    "@arcblock/ux": "^2.9.1",
    "@blocklet/sdk": "v1.16.19",
    "@blocklet/ui-react": "^2.9.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/lab": "^5.0.0-alpha.159",
    "@mui/material": "^5.15.3",
    "@nedb/core": "^2.1.5",
    "axios": "^1.6.4",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv-flow": "^3.3.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.5.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1"
  }
}
